
<p>
  Cafe for the meeting
</p>
<p>
  <%= @meeting.shop.name%>
</p>
<p>
  address:<%= @meeting.shop.address%>
</p>




<%= form_for @meeting do |f| %>
<p>
  <%= f.label :user_id %>
  <%= f.email_field :user_id, :required => true %>
</p>
<p>
  <%= f.label :shop_id %>
  <%= f.text_field :shop_id, :required => true %>
</p>
<p>
  <%= f.label :meeting_date_time %>
  <%= f.text_field :meeting_date_time, :required => true %>
</p>

<%= f.submit "Take it now"%>
<% end %>
